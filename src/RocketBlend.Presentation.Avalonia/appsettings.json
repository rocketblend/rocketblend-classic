{
  "Logging": {
    "LogFileName": "RocketBlendLog-{Date}.txt",
    "LimitBytes": 10485760,
    "DefaultLogLevel": "Debug",
    "MicrosoftLogLevel": "Debug"
  },
  "Themes": {
    "ThemeToResourceMapping": {
      "Dark": "DarkThemeName",
      "Light": "LightThemeName"
    },
    "DefaultTheme": "Dark"
  },
  "Languages": {
    "AvailableLocales": [
      "en"
    ]
  }
}